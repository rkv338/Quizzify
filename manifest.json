{
    "manifest_version": 1,
    "name": "Quizzify",
    "version": "1.0",
    "permissions": [
        "contextMenus"
    ],
    "description": "Web extension that allows users to create a quiz based on any text data they highlight",
  
    "icons": {
      "48": "icons/border-48.png"
    },
  
    "content_scripts": [
      {
        "matches": ["*"],
        "js": ["content.js"]
      }
    ]
  }